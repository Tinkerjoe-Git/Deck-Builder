<%= form_with(model: @deck) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :creator %>
    <%= f.text_field :creator %><br>
    <%= f.label :format %>
    <%= f.text_field :format %><br>
    <%= f.label :main_board %>
    <%= f.text_field :main_board %><br>
    <%= f.label :side_board %>
    <%= f.text_field :side_board %><br>
  
    
    <%= f.submit button_name %>
<% end %>